{
  "services": {
    "SimService": {
      "strategy": {
        "name": "filter",
        "filters": [
          {
            "endpoint": "name1",
            "message": {
              "fieldName1": {
                "value": "someValue1",
                "operation": "EQUAL"
              },
              "fieldName2": {
                "value": "someValue2",
                "operation": "EQUAL"
              }
            }
          },
          {
            "endpoint": "name2",
            "message": {
              "fieldName3": {
                "value": "someValue3",
                "operation": "EQUAL"
              }
            }
          }
        ]
      },
      "service-class": "com.exactpro.th2.grpc.configuration.test.TestService",
      "endpoints": {
        "name1": {
          "host": "127.0.0.1",
          "port": 5001
        },
        "name2": {
          "host": "127.0.0.2",
          "port": 5002
        }
      }
    },
    "ActService": {
      "strategy": {
        "name": "robin",
        "endpoints": [
          "name3",
          "name4"
        ]
      },
      "service-class": "com.exactpro.th2.grpc.configuration.test.TestService",
      "endpoints": {
        "name3": {
          "host": "127.0.0.3",
          "port": 5003
        },
        "name4": {
          "host": "127.0.0.4",
          "port": 5004
        }
      }
    }
  },
  "server": {
    "host": "test_host",
    "port": "1000",
    "workers": 5
  }
}
